<?xml version="1.0" encoding="utf-8"?>
<com.example.crystal.addressbook.Tab.CheckableLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:descendantFocusability="blocksDescendants"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <ImageView
        android:id="@+id/ivReceive"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_weight="0.43"
        app:srcCompat="@mipmap/ic_launcher" />

    <TextView
        android:id="@+id/tvPhone"
        android:layout_width="170dp"
        android:layout_height="50dp"
        android:layout_weight="1" />

    <CheckBox
        android:id="@+id/checkBox"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="0.26"
        android:clickable="false"
        android:focusable="false"></CheckBox>

</com.example.crystal.addressbook.Tab.CheckableLinearLayout>
